+//! when the host requests status from the device.
+//!
+//! \return None.
+//
+//*****************************************************************************
+void
+USBDCDCPowerStatusSet(void *pvCDCDevice, uint8_t ui8Power)
+{
+    ASSERT(pvCDCDevice);
+
+    //
+    // Pass the request through to the lower layer.
+    //
+    USBDCDPowerStatusSet(0, ui8Power);
+}
+#endif

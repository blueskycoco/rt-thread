+					}
+				}
+				else
+				{
+					if((g_conf.config[data[0]-33]&CONFIG_TCP)==CONFIG_TCP)
+					{					
+						g_conf.config[data[0]-33]&=~CONFIG_TCP;
+						g_chang[dev].mode=1;
+					}
+				}
+			}
+		}
+		break;
+		default:

+    // that is in the process of enumeration.
+    //
+    uint8_t ui8EnumIdx;
+
+    //
+    // The state of each of the ports we support on the hub.
+    //
+    tHubPort psPorts[MAX_USB_DEVICES];
+
+    //
+    // The interrupt number for this instance.
+    //
+    uint32_t ui32IntNum;
+};

+//*****************************************************************************
+//
+// The HID class descriptor table.  For the gamepad class there is only a
+// single report descriptor.
+//
+//*****************************************************************************
+static const uint8_t *g_ppui8GameClassDescriptors[] =
+{
+    g_pui8GameReportDescriptor
+};

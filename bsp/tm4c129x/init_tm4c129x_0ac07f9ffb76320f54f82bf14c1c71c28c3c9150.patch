+        print "Server Socket Created......."
+        tcpT4Server.bind((self.h, self.p))
+        print "Wating for connecting......."
+        tcpT4Server.listen(5)
+        while True:
+            clientSock, clientaddr = tcpT4Server.accept()
+            print "Connected from: ", clientSock.getpeername()             

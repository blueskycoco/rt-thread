+			rt_kprintf("%d remote ip4 changed\n",dev);
+		if(g_chang[dev].rip6c)
+			rt_kprintf("%d remote ip6 changed\n",dev);
+		if(g_chang[dev].rpc)
+			rt_kprintf("%d remote port changed\n",dev);

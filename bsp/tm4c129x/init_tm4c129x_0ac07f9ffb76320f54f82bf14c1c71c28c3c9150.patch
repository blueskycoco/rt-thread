+			}
+			rt_free(tmp);
+		}
+		break;
+		case 7:
+		{
+			//set mac
+			
+		}
+		break;
+		case 8:
+		case 9:
+		case 10:
+		case 11:
+		{
+			//set remote ipv4 ip

+        //
+        psBulkDevice->pfnRxCallback(psBulkDevice->pvRxCBData,
+                                    USB_EVENT_DISCONNECTED, 0, (void *)0);
+    }
+
+    //
+    // Remember that we are no longer connected.
+    //
+    psInst->bConnected = false;
+}

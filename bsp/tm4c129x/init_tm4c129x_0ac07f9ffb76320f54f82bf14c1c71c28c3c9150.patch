+static uint32_t HIDMouseRxHandler(void *pvMouseDevice, uint32_t ui32Event,
+                                  uint32_t ui32MsgData, void *pvMsgData);
+static uint32_t HIDMouseTxHandler(void *pvMouseDevice, uint32_t ui32Event,
+                                  uint32_t ui32MsgData, void *pvMsgData);

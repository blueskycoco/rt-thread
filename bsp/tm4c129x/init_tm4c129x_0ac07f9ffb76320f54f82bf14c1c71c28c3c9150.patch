+        print "Server Socket Created......."
+        tcpT6Server.bind((self.h, self.p))
+        print "Wating for connecting......."
+        tcpT6Server.listen(5)
+        while True:
+            clientSock, clientaddr = tcpT6Server.accept()
+            print "Connected from: ", clientSock.getpeername() 
+            while True:

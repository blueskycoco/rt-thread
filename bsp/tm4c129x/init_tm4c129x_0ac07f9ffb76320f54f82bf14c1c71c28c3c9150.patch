+    //! This is a keyboard device.
+    //
+    eUSBHHIDClassKeyboard,
+
+    //
+    //! This is a mouse device.
+    //
+    eUSBHHIDClassMouse,
+
+    //
+    //! This is a vendor specific device.
+    //
+    eUSBHHIDClassVendor
+}
+tHIDSubClassProtocol;

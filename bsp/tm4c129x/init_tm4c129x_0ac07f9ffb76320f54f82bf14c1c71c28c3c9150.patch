+					rt_sprintf(g_conf.remote_ip[data[0]-8],"%d.%d.%d.%d",data[1],data[2],data[3],data[4]);
+					g_chang[dev].rip4c=1;
+				}
+				rt_free(tmp);
+			}
+		}
+		break;
+		case 12:
+		case 13:			
+		case 14:
+		case 15:
